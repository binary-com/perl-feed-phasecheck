sub is_approx_num {
    my ($num1, $num2, $msg, $tolerance) = @_;

    $num1 =~ /^(\d+).+?e-(\d+)/;
    my ($val1, $e1) = ($1, $2);

    $num2 =~ /^(\d+).+?e-(\d+)/;
    my ($val2, $e2) = ($1, $2);

    unless (ok($val1 == $val2 && $e1 == $e2, $msg)) {
        diag("'$num1' =~ '$num2'");
    }
}
